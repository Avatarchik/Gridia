<ContainerView>
  <List Id="Container" Style="ShowcaseList" Orientation="Horizontal" Items="{Items}" CanSelect="True" Spacing="3" Alignment="{Alignment}" Overflow="Wrap">
    <ListItem Id="Template" Width="35px" Height="35px" Selected-OutlineColor="Yellow" IsTemplate="True">
      <ItemView ItemInstance="{#Item}" />
    </ListItem>
  </List>
</ContainerView>
